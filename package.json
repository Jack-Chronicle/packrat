{
  "name": "packrat-plugin",
  "version": "1.1.2",
  "description": "Delete, archive, or move completed recurring Tasks based on in-line triggers",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "obsidian-tasks",
    "task-manager",
    "task-management",
    "completed recurring tasks",
    "completed tasks"
  ],
  "author": "therden",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "babel-jest": "^29.7.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "obsidian": "latest",
    "ts-jest": "^29.1.2",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
